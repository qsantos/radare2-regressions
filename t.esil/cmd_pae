NAME='udis pae 0xed31'
FILE=-
CMDS='
e asm.arch = x86.udis
pae 31ed
'
EXPECT='ebp,ebp,^=
'

run_test


NAME='capstone pae ed31'
FILE=-
BROKEN=
CMDS='
pae 31ed
'
EXPECT='ebp,ebp,^=,%z,zf,=,%p,pf,=,0,cf,=,0,of,=,%s,sf,=
'

run_test
