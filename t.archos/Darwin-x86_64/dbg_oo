#!/bin/sh

# TODO: This script requires ROOT to be run in OSX
# (or just sign the r2 binary to get debugger priviledges)
# GPL 3+ - Copyright (C) 2014  pancake

. $(git rev-parse --show-toplevel)/tests.sh

NAME='dbg.oo 10'
FILE=/bin/ls
ARGS=-d
BROKEN=
CMDS='10oo
e file.path
'
EXPECT='dbg:///bin/ls
'
IGNORE_ERR=1
run_test

NAME='oo ; dc'
FILE=../../bins/elf/x86-helloworld-gcc
ARGS=-d
BROKEN=1
CMDS='oo
dc
'
EXPECT='Hello world!
'
IGNORE_ERR=1
run_test